[core]
    editor = vim

[color]
    ui = true
    diff = auto
    log = auto
    commit = auto
    branch = auto
    status = auto
    interactive = auto

[color "branch"]
    current = green reverse
    local = cyan
    remote = blue

[color "diff"]
    meta = white bold
    frag = yellow bold
    old = red
    new = green

[color "status"]
    added = green
    changed = yellow
    untracked = blue
    branch = white bold
