defutf8 on
defscrollback 8192
autodetach on
startup_message off
hardcopydir ~/.screen
hardstatus on
hardstatus alwayslastline
hardstatus string '%{= kG}[ %{G}%H %{g}][%= %{= kw}%?%-Lw%?%{r}(%{W}%n*%f%t%?(%u)%?%{r})%{w}%?%+Lw%?%?%= %{g}][%{B} %y:%m:%d:%{W}%c:%s %{g}]'
nethack on
zombie kr

screen -t irc ssh -t shell.cat screen -DRUAa
screen -t cat ssh -t shell.cat ssh -t miranda screen -DRUAa
screen -t school ssh -t csland /opt/csw/bin/screen -DRUAa
screen -t powah su -
screen -t music
screen
select 0
